    <defName>NamerFactionBorg</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name-&gt;[BorgFactionNames]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>BorgFactionNames</keyword>
          <path>NameBanks/BorgFactionNames</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
